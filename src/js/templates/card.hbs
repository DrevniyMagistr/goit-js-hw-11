{{#each this}}
  <a class='image-link link' href='{{largeImageURL}}'>
    <div class='photo-card'>
      <img src='{{webformatURL}}' alt='{{htags}}' loading='lazy' class='img-item' />
      <div class='info'>
        <p class='info-item'>
          <span>Likes:</span>{{likes}}
        </p>
        <p class='info-item'>
          <span>Views:</span>{{views}}
        </p>
        <p class='info-item'>
          <span>Comments:</span>{{comments}}
        </p>
        <p class='info-item'>
          <span>Downloads:</span>{{downloads}}
        </p>
      </div>
    </div>
  </a>
{{/each}}